{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "connect-src 'self' https://npchamnsaaykeedohfli.supabase.co https://npchamnsaaykeedohfli.functions.supabase.co https://riddimbase-backend.onrender.com https://riddimbaseorigin.s3.us-east-2.amazonaws.com wss://npchamnsaaykeedohfli.supabase.co;"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sitemaps/pages.xml",
      "destination": "/api/sitemap-pages"
    },
    {
      "source": "/sitemaps/beats.xml",
      "destination": "/api/sitemap-beats"
    },
    {
      "source": "/sitemaps/producers.xml",
      "destination": "/api/sitemap-producers"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
